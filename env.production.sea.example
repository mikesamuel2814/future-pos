# Sea POS - Production Environment Variables
# Copy to /var/www/seapos/.env.production on the server (or use deploy script)

# Database
DATABASE_URL=postgresql://seapos_user:SeaPOS2024!Secure@localhost:5432/seapos_db

# Server
PORT=7070
NODE_ENV=production

# Session - generate: openssl rand -base64 32
SESSION_SECRET=your-secure-session-secret-key-change-this-in-production

# Central Dashboard API (for reporting)
CENTRAL_DASHBOARD_API_KEY=central-dashboard-2024-secure-key
